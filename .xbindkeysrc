#keystate_capslock = enable



#Raise Volumecde
"pulseaudio-ctl up"
    m:0x0 + c:123
    XF86AudioRaiseVolume 

#Lower Volume
"pulseaudio-ctl down"
    m:0x0 + c:122
    XF86AudioLowerVolume 

#Mute
"pulseaudio-ctl mute-toggle"
    m:0x0 + c:121
    XF86AudioMute 

#NExt
"mpc next"
    m:0x0 + c:171
    XF86AudioNext 

#Previous
"mpc prev"
    m:0x0 + c:173
    XF86AudioPrev 

#Pause
"mpc toggle"
    m:0x0 + c:215
    XF86AudioPlay 

